# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
POST  /file          controllers.ScalaFileUploadController.upload()
GET     /                              controllers.Pages.login


GET     /charts/flot                  controllers.Charts.flot
GET     /bouneb                  controllers.Charts.bouneb
GET     /charts/morris                controllers.Charts.morris

GET     /ui-elements/panels-and-wells controllers.UiElements.panelsAndWells
GET     /ui-elements/buttons          controllers.UiElements.buttons
GET     /ui-elements/notifications    controllers.UiElements.notifications
GET     /ui-elements/typography       controllers.UiElements.typography
GET     /ui-elements/icons            controllers.UiElements.icons
GET     /ui-elements/grid             controllers.UiElements.grid
GET     /pages/blank                  controllers.Pages.blank
GET     /pages/login                  controllers.Pages.login

GET     /select         controllers.Select.segmentation  
GET     /join         controllers.Select.segmentation1 
GET     /groupby         controllers.Select.segmentation2                                                                                                                                               
GET     /cols         controllers.Select.segmentationcol 
GET     /todataframe        controllers.Select.filetodataframe

GET     /rename         controllers.Select.rename  
                                                                                                                                            
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET     /tesr                           controllers.UserData.index
GET     /tesrchurn                           controllers.UserData.index1
GET     /tesr1                           controllers.TimeData.index

GET     /createUser                     controllers.UserData.createUser
GET     /createUser1                     controllers.UserData.createUser1
GET     /createUser2                     controllers.UserData.createUser2
GET     /createTime1                     controllers.TimeData.createTime1

